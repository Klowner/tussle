{
	"name": "@tussle/storage-s3",
	"version": "0.4.5",
	"description": "Tussle storage component backed by S3 compatible storage",
	"main": "./dist-cjs/index.js",
	"types": "./dist-types/index.d.ts",
	"module": "./dist-es/index.js",
	"files": [
		"dist-cjs",
		"dist-types",
		"dist-es"
	],
	"scripts": {
		"build": "concurrently 'yarn:build:es' 'yarn:build:types' 'yarn:build:cjs'",
		"build:es": "tsc -p tsconfig.es.json",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:types": "tsc -p tsconfig.types.json",
		"clean": "rimraf dist-cjs dist-es dist-types coverage",
		"test": "jest"
	},
	"author": "Mark Riedesel <mark@klowner.com>",
	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.58.0",
		"@tussle/core": "^0.4.5",
		"rxjs": "^7.0.0"
	},
	"devDependencies": {
		"@tussle/spec": "^0.4.5",
		"concurrently": "^7.4.0",
		"jest": "^29.1.2",
		"nock": "^13.2.4",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"typescript": "^4.8.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	}
}
