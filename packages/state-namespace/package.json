{
	"name": "@tussle/state-namespace",
	"version": "0.6.12",
	"description": "Wrapper for Tussle State to add transparent key namespacing",
	"main": "dist-cjs/index.js",
	"types": "dist-types/index.d.ts",
	"module": "dist-es/index.js",
	"files": [
		"dist-cjs",
		"dist-types",
		"dist-es"
	],
	"scripts": {
		"build": "concurrently 'yarn:build:es' 'yarn:build:types' 'yarn:build:cjs'",
		"build:es": "tsc -p tsconfig.es.json",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:types": "tsc -p tsconfig.types.json",
		"watch": "tsc -b --watch",
		"clean": "rimraf dist-cjs dist-types dist-es coverage tsconfig.tsbuildinfo",
		"test": "jest --coverage"
	},
	"author": "Mark Riedesel <mark@klowner.com>",
	"license": "MIT",
	"devDependencies": {
		"@tussle/core": "^0.6.12",
		"@tussle/spec": "^0.6.12",
		"@tussle/state-memory": "^0.6.12",
		"concurrently": "^7.5.0",
		"jest": "^29.2.2",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"typescript": "^4.8.4"
	},
	"publishConfig": {
		"access": "public"
	}
}
