{
  "name": "@klowner/tussle",
  "version": "1.0.0",
  "description": "summon your own tus daemon",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "run-s clean && run-p build:lib",
    "build:lib": "tsc -b ./packages/tsconfig.json",
    "watch": "yarn run build:lib --watch --preserveWatchOutput",
    "bootstrap": "lerna exec yarn install",
    "clean": "lerna run --parallel clean",
    "release": "lerna publish"
  },
  "keywords": [],
  "author": "Mark Riedesel <mark@klowner.com>",
  "license": "MIT",
  "devDependencies": {
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.9.7"
  },
  "private": true,
  "dependencies": {
    "@klowner/tussle-core": "file:packages/tussle-core",
    "@klowner/tussle-koa": "file:packages/tussle-koa"
  }
}
